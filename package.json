{
    "name": "notebook",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "npm-run-all --parallel watch:build watch:server admin:server",
        "watch:build": "webpack --watch --mode=development",
        "watch:server": "nodemon -e js,twig server.js --exec babel-node --presets es2015",
        "admin:server": "npm start --prefix admin",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "nodemonConfig": {
        "ignore": [
            "admin/*",
            "client/www/*",
            "client/styles/*"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.16.2",
        "express-http-proxy": "^1.2.0",
        "moment": "^2.22.0",
        "mysql2": "^1.5.3",
        "randomstring": "^1.1.5",
        "sequelize": "^4.37.4",
        "twig": "^1.10.5"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-es2015": "^6.24.1",
        "body-parser": "^1.18.2",
        "css-loader": "^0.28.11",
        "file-loader": "^1.1.11",
        "mini-css-extract-plugin": "^0.4.0",
        "node-fetch": "^2.1.2",
        "node-sass": "^4.8.3",
        "nodemon": "^1.17.1",
        "npm-run-all": "^4.1.2",
        "sass-loader": "^7.0.1",
        "twig-loader": "^0.3.1",
        "webpack": "^4.6.0",
        "webpack-cli": "^2.0.11"
    }
}
